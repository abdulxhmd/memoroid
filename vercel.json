{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ],
    "functions": {
        "app/api/generate/route.ts": {
            "runtime": "nodejs18.x",
            "memory": 1024,
            "maxDuration": 30
        }
    }
}